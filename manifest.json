{
    "manifest_version": 2,
    "name": "PasswordZen",
    "version": "1.0",
    "content-scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "Popup.html",
        "default_title": "PasswordZen"
    }
}